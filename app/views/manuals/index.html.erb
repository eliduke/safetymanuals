<p id="notice"><%= notice %></p>

<h1>Manuals</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Make</th>
      <th>Model</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manuals.each do |manual| %>
      <tr>
        <td><%= manual.company %></td>
        <td><%= manual.make %></td>
        <td><%= manual.model %></td>
        <td><img src="<%= manual.photo.url %>"></td>
        <td><%= link_to 'Show', manual %></td>
        <td><%= link_to 'Edit', edit_manual_path(manual) %></td>
        <td><%= link_to 'Destroy', manual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manual', new_manual_path %>
